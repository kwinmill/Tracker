require 'Parade.rb'
require 'Photographer.rb'
require 'Order.rb'
class Home
  private
    @home_id
    @home_name
    @address
    @city
    @state
    @notes
    @parade
    @order
    @photographer
    @builder
  public
    def initialize(home = "-1",name="empty",address="empty",city="empty",state="empty",notes="empty",parade="empty",order="empty",photographer="empty",builder="empty")
      @home_id = home
      @home_name=name
      @address = address
      @city = city
      @state = state
      @notes = notes
      @parade = parade
      @order = order
      @photographer = photographer
      @builder = builder
    end
    def self.set_home_id(id)
      @home_id=id
    end
    def self.get_home_id
      @home_id
    end
    def self.set_home_name(name)
      @home_name = name
    end
    def self.get_home_name
      @home_name
    end
    def self.set_home_address(address)
      @address=address
    end
    def self.get_home_address
      @address
    end
    def self.set_city(city)
      @city=city
    end
    def self.get_city
      @city
    end
    def self.set_state(state)
      @state=state
    end
    def self.get_state
      @state
    end
    def self.set_home_notes(notes)
      @notes=notes
    end
    def self.get_home_notes
      @notes
    end
    def self.get_parade
      Parade
    end
end
